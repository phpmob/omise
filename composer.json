{
    "name": "phpmob/omise",
    "description": "Just alternative Omise api client",
    "keywords": [
        "omise",
        "omise-api"
    ],
    "homepage": "https://github.com/phpmob/omise",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpMob\\Omise\\": "src"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\PhpMob\\Omise\\": "spec/",
            "tests\\PhpMob\\Omise\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/discovery": "^1.0",
        "symfony/options-resolver": "^3.3",
        "doctrine/inflector": "^1.1"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.1",
        "phpspec/phpspec": "^4.0",
        "phpunit/phpunit": "^5.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "php-http/guzzle6-adapter": "To use build-in \\PhpMob\\Omise\\Client\\GuzzleHttpClient."
    }
}
